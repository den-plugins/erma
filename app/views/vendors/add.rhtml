<%= render :partial => '/admin/menu' %>

<h2><%= html_title(l(:label_erma_add, :tab => 'Vendor')) %></h2>

<% labelled_tabular_form_for :vendor, @vendor, :url => { :action => "add" } do |f| %>
  
  <%= error_messages_for 'vendor' %>

  <%= render :partial => 'form', :locals => { :f => f } %>

  <%= submit_tag l(:button_save) %> | <%= link_to 'Cancel', :controller => "vendors" %>
  
<% end %>
