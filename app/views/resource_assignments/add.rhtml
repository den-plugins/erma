<%= render :partial => '/admin/menu' %>

<h2><%=html_title(l(:label_erma_add_resource_assignment))%></h2>

<% labelled_tabular_form_for :resource_assignment, @resource_assignment, :url => { :action => "add" } do |f| %>
<%= render :partial => 'form', :locals => { :f => f, :resources => @resources } %>

<%= submit_tag l(:button_add) %> | <%= link_to 'Cancel', :controller => 'statement_of_works', :action => 'show', :id => @resource_assignment.statement_of_work.id %>
<% end %>
