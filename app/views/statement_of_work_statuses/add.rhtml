<%= render :partial => '/admin/menu' %>

<h2><%=html_title(l('New Status'))%></h2>

<% labelled_tabular_form_for :status, @status, :url => { :action => "add" } do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>

<%= submit_tag l(:button_add) %> | <%= link_to 'Cancel', :back %>
<% end %>
